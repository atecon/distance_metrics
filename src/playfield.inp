matrix M = seq(-3,3)'
M ~= M.^2
M

eval onenorm(M)			# L-1
eval onenorm(M.^2)		# L-2
eval infnorm(M)

matrix m = {3,-2,1}'
eval onenorm(m)
eval sqrt(onenorm(m))
eval onenorm(m.^2)
eval sqrt(onenorm(m.^2)) 	# Euclidean norm of vector

scalar N = 10^6
matrix BIG = mnormal(N,10)

loop i=1..2 -q
    
    set stopwatch
    if i==1
        scalar n = sum(BIG.^2)
    else
        scalar n = onenorm(BIG.^2)
    endif
    
    printf "For i==$i this took %g sec.\n", $stopwatch

endloop
